{
  "id": null,
  "title": "Autonomous Living - Goals Dashboard",
  "tags": [
    "autonomous-living",
    "goals"
  ],
  "style": "dark",
  "timezone": "browser",
  "refresh": "1m",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "Overall Progress",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "autonomous_tasks_completed / autonomous_tasks_total * 100",
          "legendFormat": "Completion Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          },
          "mappings": [],
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "id": 2,
      "title": "Tasks by Priority",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "autonomous_tasks_priority",
          "legendFormat": "{{priority}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "vis": false,
              "legend": false
            }
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Goal Progress",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "autonomous_goal_progress",
          "legendFormat": "{{name}}",
          "refId": "A",
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "include": [
              "goal",
              "name",
              "phase",
              "priority",
              "Value"
            ]
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            }
          },
          "thresholds": {
            "steps": [
              {
                "color": "super-light-red",
                "value": null
              },
              {
                "color": "semi-dark-red",
                "value": 30
              },
              {
                "color": "semi-dark-yellow",
                "value": 60
              },
              {
                "color": "semi-dark-green",
                "value": 80
              }
            ]
          },
          "mappings": [],
          "color": {
            "mode": "thresholds"
          }
        }
      },
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 4,
      "title": "Task Completion by Goal",
      "type": "bargauge",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "autonomous_goal_completion_rate",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        }
      }
    },
    {
      "id": 5,
      "title": "Total Tasks Overview",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      },
      "targets": [
        {
          "expr": "autonomous_goals_total",
          "legendFormat": "Total Goals",
          "refId": "A"
        },
        {
          "expr": "autonomous_tasks_total",
          "legendFormat": "Total Tasks",
          "refId": "B"
        },
        {
          "expr": "autonomous_tasks_completed",
          "legendFormat": "Completed Tasks",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "colorMode": "background"
      }
    },
    {
      "type": "table",
      "title": "Goal G01: Target Body Fat - Incomplete Tasks",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 26
      },
      "id": 6,
      "datasource": "GoalsJSON",
      "targets": [
        {
          "target": "goals",
          "refId": "A",
          "data": "const goalId = 'G01';\nconst incompleteTasks = data[goalId].tasks.filter(task => !task.completed);\n\nreturn incompleteTasks.map(task => ({\n  'Task ID': task.id,\n  'Task Name': task.name,\n  'Priority': task.priority,\n  'Time Estimate (m)': task.minutes\n}));"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
